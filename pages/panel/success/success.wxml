<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{title}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn tc-btn-primary" type="primary" bindtap="goLogin">现在去登录</button>
                <button class="weui-btn" type="default" bindtap="goIndex">首页</button>
            </view>
        </view>
    </view>
</view>